<Window x:Class="GeverProject.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GeverProject"
        mc:Ignorable="d"
        Title="Use VPNS" Height="450" Width="800">
    <Grid>
        <Label Content="Choose And Start" HorizontalAlignment="Left" Height="44" Margin="299,36,0,0" VerticalAlignment="Top" Width="202" FontSize="24"/>
        <ListBox x:Name="lb" HorizontalAlignment="Left" Height="48" Margin="299,114,0,0" VerticalAlignment="Top" Width="202">
            <ListBoxItem Content="NordVPN"/>
            <ListBoxItem Content="Express VPN"/>
            <ListBoxItem Content="WindScribe VPN"/>
        </ListBox>
        <TextBox x:Name="username" HorizontalAlignment="Left" Height="30" Margin="299,209,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="202"/>
        <PasswordBox HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <PasswordBox x:Name="password" HorizontalAlignment="Left" Height="30" Margin="299,257,0,0" VerticalAlignment="Top" Width="202"/>
        <Button Content="Start" HorizontalAlignment="Left" VerticalAlignment="Top" Width="202" Margin="299,321,0,0" Height="40" FontSize="22" Click="Button_Click" IsDefault="True"/>

    </Grid>
</Window>
